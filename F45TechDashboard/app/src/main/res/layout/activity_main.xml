<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:weightSum="2"
    tools:context="com.android.f45tv.f45techdashboard.MainActivity">


    <RelativeLayout
        android:layout_width="653dp"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:weightSum="1"
        android:background="">

        <include
            android:id="@+id/main_45logo"
            android:layout_width="177dp"
            android:layout_height="147dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="14dp"
            android:layout_marginTop="17dp"
            android:layout="@layout/f45logo" />

        <include
            android:id="@+id/main_avgResponse"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/main_ticketsToday"
            android:layout_alignParentTop="true"
            android:layout_marginTop="119dp"
            android:layout="@layout/response" />

        <include
            android:id="@+id/main_ticketsToday"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="216dp"
            android:layout_marginTop="78dp"
            android:layout="@layout/tickets" />

        <include
            android:id="@+id/main_scheduleview"
            android:layout_width="208dp"
            android:layout_height="164dp"
            android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true"
            android:layout_marginEnd="65dp"
            android:layout="@layout/schedule" />
    </RelativeLayout>


</LinearLayout>
