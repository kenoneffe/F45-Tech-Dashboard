<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.android.f45tv.f45techdashboard.MainActivity">


    <include
        android:id="@+id/main_45logo"
        android:layout_width="wrap_content"
        android:layout_height="115dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="24dp"
        android:layout="@layout/f45logo" />

    <include
        android:id="@+id/main_ticketsToday"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="186dp"
        android:layout_marginTop="31dp"
        android:layout="@layout/tickets" />


    <include
        android:id="@+id/main_avgResponse"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/main_ticketsToday"
        android:layout_marginTop="72dp"
        android:layout="@layout/response" />

    <include
        android:id="@+id/main_scheduleview"
        android:layout_width="245dp"
        android:layout_height="118dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout="@layout/schedule" />

    <include
        android:id="@+id/main_marque_scrolling_text"
        layout="@layout/marquee"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="125dp" />
</RelativeLayout>
